{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "overrides": [
        {
            "files": "*.{less,sass,scss,css}",
            "options": {
                "singleQuote": false
            }
        }
    ]
}
